@model CheeseMVC.ViewModels.ViewMenuViewModel


<h3>Menu @Model.Menu.Name cheeses:</h3>


<ul>
    @foreach (var cheeseMenu in Model.Items)
    {
        <li>@cheeseMenu.Cheese.Name</li>
    }
</ul>






@if (Model.Items.Count == 0)
{
    <p><i>No Menu Cheeses</i></p>
}


<p><a asp-controller="Menu" asp-action="AddItem" asp-route-id="@Model.Menu.ID">Add Cheese to Menu</a></p>


